{
  "name": "sungrow-components",
  "version": "0.0.3",
  "description": "basic component for sungorw",
  "main": "lib/sungrow-components.common.js",
  "author": "yaokunlun@sungrowpower.com",
  "license": "MIT",
  "scripts": {
    "ready": "npm install && npm run build:file && npm run start",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.example.js",
    "build:file": "node build/bin/gen-components & node build/bin/build-entry.js",
    "build:theme": "gulp build --gulpfile packages/sungrow-theme/gulpfile.js && cp-cli packages/sungrow-theme/lib lib/sungrow-theme",
    "clean": "rimraf lib",
    "dist": "npm run build:file && webpack --config build/webpack.common.js && webpack --config build/webpack.component.js && npm run build:theme"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@vue/component-compiler-utils": "^3.1.2",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "copy-webpack-plugin": "^6.0.2",
    "cp-cli": "^2.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.6.0",
    "url-loader": "^1.0.1",
    "file-loader": "^1.1.11",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.1.0",
    "highlight.js": "^10.1.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "js-cookie": "^2.2.1",
    "json-templater": "^1.2.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
